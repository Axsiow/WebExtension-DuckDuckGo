{
    "manifest_version": 2,
    "name": "ChangerDevelopperTwitter",
    "version": "1.0",
  
    "description": "Change design for developer.twitter",
  
    "icons": {
      "48": "icons/square-twitter-48.png",
      "96": "icons/square-twitter-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.developer.twitter.com/*"],
        "js": ["twitter.js"]
      }
    ]
  
  }